{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"additionalProperties": false,
	"required": [
		"email",
		"lastname",
		"firstname"
	],
	"properties": {
		"id": {
			"type": "string",
			"readOnly": true
		},
		"email": {
			"type": "string",
			"format": "email"
		},
		"lastname": {
			"type": "string"
		},
		"firstname": {
			"type": "string"
		}
	}
}