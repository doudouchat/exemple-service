{
	"$schema": "http://json-schema.org/draft-07/schema",
	"additionalProperties": false,
	"required": [
		"username",
		"password",
		"id"
	],
	"properties": {
		"username": {
			"type": "string",
			"login": true
		},
		"id": {
			"type": "string",
			"createOnly": true
		},
		"password": {
			"type": "string",
			"writeOnly": true
		},
		"plain_password": {
			"type": "string"
		},
		"enable": {
			"type": "boolean"
		}
	}
}